<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="task009.ComplexMatrix2x2" id="matrix2x2">
    </bean>

    <bean class="task009.ComplexMatrix2x2" id="matrixOneParam">
        <constructor-arg ref="number1"></constructor-arg>
    </bean>

    <bean class="task009.ComplexMatrix2x2" id="zeroMatrix">
        <constructor-arg ref="zeroNumber"></constructor-arg>
    </bean>

    <bean class="task009.ComplexMatrix2x2" id="matrixWithFirthNumber">
        <constructor-arg ref="number3"></constructor-arg>
    </bean>

    <bean class="task009.ComplexMatrix2x2" id="matrixWithFourthNumber">
        <constructor-arg ref="number4"></constructor-arg>
    </bean>


    <bean class="task009.ComplexMatrix2x2" id="matrixFourParams">
        <constructor-arg ref="number1"></constructor-arg>
        <constructor-arg ref="number2"></constructor-arg>
        <constructor-arg ref="number3"></constructor-arg>
        <constructor-arg ref="number4"></constructor-arg>
    </bean>

    <bean class="task009.ComplexNumber" id="zeroNumber">
        <property name="a" value="0"></property>
        <property name="b" value="0"></property>
    </bean>

    <bean class="task009.ComplexNumber" id="number1">
        <property name="a" value="2"></property>
        <property name="b" value="4"></property>
    </bean>
    <bean class="task009.ComplexNumber" id="number2">
        <property name="a" value="2"></property>
        <property name="b" value="1"></property>
    </bean>
    <bean class="task009.ComplexNumber" id="number3">
        <property name="a" value="5"></property>
        <property name="b" value="4"></property>
    </bean>
    <bean class="task009.ComplexNumber" id="number4">
        <property name="a" value="1"></property>
        <property name="b" value="1"></property>
    </bean>
    <bean class="task009.ComplexNumber" id="vectorFirstNumber">
        <constructor-arg value="-11.0"></constructor-arg>
        <constructor-arg value="19.0"></constructor-arg>
    </bean>
    <bean class="task009.ComplexNumber" id="vectorSecondNumber">
        <constructor-arg value="-6.0"></constructor-arg>
        <constructor-arg value="30.0"></constructor-arg>
    </bean>


    <bean class="task009.ComplexNumber" id="multNumber">
        <property name="a" value="2"></property>
        <property name="b" value="18"></property>
    </bean>
    <bean class="task009.ComplexNumber" id="sumNumber">
        <property name="a" value="4"></property>
        <property name="b" value="5"></property>
    </bean>
    <bean class="task009.ComplexNumber" id="detNumber">
        <property name="a" value="-8"></property>
        <property name="b" value="-7"></property>
    </bean>

    <bean class="task009.ComplexVector2D" id="multVector">
        <constructor-arg ref="vectorFirstNumber"></constructor-arg>
        <constructor-arg ref="vectorSecondNumber"></constructor-arg>
    </bean>

    <bean class="task009.ComplexVector2D" id="complexVector">
        <constructor-arg ref="number1"></constructor-arg>
        <constructor-arg ref="number4"></constructor-arg>
    </bean>


</beans>